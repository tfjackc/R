# load packages -----------------
library(tidyverse)
library(here)
library(sf)
library(ggplot2)
library(leaflet)
# load my own data ---------------
bikeRide <- read_csv(here("data", "gpx_geoDF.csv"))
# View(bikeRide)
# create a ggplot
ggplot(bikeRide, aes(x = longitude, y = latitude)) +
geom_point()
# Create a leaflet object.
m <- leaflet() %>%
addTiles()
# Add the ggplot object to the leaflet object.
m <- m %>%
addMarkers(data = bikeRide, lat = ~latitude, lng = ~longitude)
# Display the leaflet object.
m
